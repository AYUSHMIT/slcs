Let bigStreet = [~ # 248 213 169];
Let midStreet = [~ # 247 247 186];

Let street = 
    C^3 (bigStreet | midStreet);

Let stadium = [~ # 51 204 153];

Let stadiumArea = 
    !((!stadium) U street);

Let streetsAroundStadium = 
    street & C^7 stadiumArea;

Paint "orange" stadiumArea;
Paint "red" streetsAroundStadium;
